    "build": "pnpm run build:lib && pnpm run build:site"
  },
  "keywords": [
    "formily",
    "schema",
    "request",
    "fetch",
    "dynamic"
  ],
  "dependencies": {
    "@formily/antd-v5": "^1.1.2",
    "@formily/core": "^2.2.27",
    "@formily/react": "^2.2.27",
    "@formily/reactive": "^2.2.27"
  }
}
