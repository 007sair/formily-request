{
  "name": "formily-request-monorepo",
  "version": "0.2.0",
  "description": "A flexible, lightweight, non-intrusive plugin for extending formily schema request data.",
  "author": "Long Chan",
  "license": "MIT",
  "homepage": "https://github.com/007sair/formily-request",
  "scripts": {
    "build:lib": "pnpm run --parallel -F formily-request build --watch",
    "storybook": "pnpm build:lib & pnpm run --parallel -F examples storybook",
    "deploy:storybook": "pnpm -F examples run deploy-storybook"
  },
  "keywords": [
    "formily",
    "schema",
    "request",
    "fetch",
    "dynamic"
  ],
  "devDependencies": {
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@formily/antd-v5": "^1.1.2",
    "@formily/core": "^2.2.27",
    "@formily/react": "^2.2.27",
    "@formily/reactive": "^2.2.27"
  }
}
